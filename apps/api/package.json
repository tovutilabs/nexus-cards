{
  "name": "@nexus-cards/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev:watch": "nest start --watch --exec 'node -r tsconfig-paths/register dist/apps/api/src/main'",

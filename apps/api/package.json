    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.32.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
